{
  "manifest_version": 3,
  "name": "CSES Enhanced",
  "version": "1.1",
  "description": "Difficulty ratings, speedrun mode, and practice tools for CSES.",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://cses.fi/*",
    "https://www.cses.fi/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://cses.fi/*"],
      "css": ["styles.css", "katex.min.css"],
      "js": ["katex.min.js", "content.js"]
    }
  ]
}